{
  "name": "Flash Link in New Tab",
  "short_name": "FLNT",
  "version": "2",
  "manifest_version": 2,
  "description": "A chrome extension for quick links in new tab.",
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks"
  ]
}